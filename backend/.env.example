# 通义千问 API配置
QWEN_API_KEY=your_qwen_api_key_here # API密钥
QWEN_API_URL=https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation # API地址
QWEN_MODEL=qwen-turbo # 模型名称

# API参数配置
MAX_TOKENS=1000 # 最大生成token数
TEMPERATURE=0.7 # 生成温度
TOP_P=0.8       # 采样范围
API_TIMEOUT=30000 # API超时时间

# 对话配置
MAX_HISTORY_MESSAGES=10 # 最大上下文长度
SYSTEM_PROMPT=你是一个有用的AI助手，请用简洁明了的中文回答用户的问题。你能够根据之前的对话内容来理解上下文，提供连贯和相关的回答。 # 系统提示词

# 服务器配置
PORT=3001
NODE_ENV=development
